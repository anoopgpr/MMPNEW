.toggle_block {
    display: block;
    width: 100%;
    position: relative;
    height: 12px;
}
.toggle_thumb {
    display: block;
    position: absolute;
    width: 12px;
    height: 12px;
    background: #fff;
    border-radius: 50%;
    top: 0;
    /*right: 0;*/
	-webkit-animation: toggle1 100ms forwards ease-in-out;
	-moz-animation: toggle1 100ms forwards ease-in-out;
	-o-animation: toggle1 100ms forwards ease-in-out;
	animation: toggle1 100ms forwards ease-in-out;
}
.mode_toggle input {
    display: none;
}


.mode_toggle input[type='checkbox']:checked + .toggle_block .toggle_thumb {
	-webkit-animation: toggle 100ms forwards ease-in-out;
	-moz-animation: toggle 100ms forwards ease-in-out;
	-o-animation: toggle 100ms forwards ease-in-out;
	animation: toggle 100ms forwards ease-in-out;
}
